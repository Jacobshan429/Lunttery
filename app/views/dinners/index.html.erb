  <div class="row">
    <div class="col-md-10 col-md-offset-1 .col-sm-12">
      <!-- <div class="row">
        <div class="col-md-2"> -->
        <%@meals.each do  |meal|%>
          <div class="meal_info">
            <% if meal.photos!=[]%>
            <div class="meal_pic" style="background-image: url('<%= meal.photo_url%>');"></div>
            <% else %>
            <div class="meal_pic"></div>            
            <% end %>
              <div class="meal_text">
                <h2><%=meal.name %></h2>
                <span class="gray">#<%=meal.style.name%></span>
                <div class="meal_price"><%=meal.price%>å…ƒ<a class="main_color to_right"><i class="fa fa-heart-o" aria-hidden="true"><%=meal.users.count%></i></a></div>
                <span class="gray"></span>
              </div>
          </div><!-- meal_info -->
        <%end %>
      <%=paginate @meals%>  
    </div>
  </div>