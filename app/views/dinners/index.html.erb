  <div class="row">
    <div class="col-md-10 col-md-offset-1 .col-sm-12">
      <!-- <div class="row">
        <div class="col-md-2"> -->
        <%@meals.each do  |meal|%>
          <div class="meal_info">
            <% if meal.photos!=[]%>
            <div class="meal_pic" style="background-image: url('<%=meal.return_photo_url%>');"></div>
            <% else %>
            <div class="meal_pic"></div>            
            <% end %>
              <div class="meal_text">
                <h2><%=meal.name %></h2>
                <span class="gray">#<%=meal.style.name%></span>
                <div class="meal_price"><%=meal.price%>元<a class="main_color to_right"><i class="fa fa-heart-o" aria-hidden="true"> 18</i></a></div>
                <span class="gray">#四平商圈</span>
              </div>
          </div><!-- meal_info -->
        <%end %>
      <%=paginate @meals%>  
    </div>
  </div>